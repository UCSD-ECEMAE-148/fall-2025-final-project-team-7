#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'blink-detector','console_scripts','blink_detector'
__requires__ = 'blink-detector'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('blink-detector', 'console_scripts', 'blink_detector')()
    )
